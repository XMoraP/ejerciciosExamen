<%- include("header", {}) %>

<table>
 <tr> 
  <th>Username
  </th>
  <th>Role
  </th>
  <th>Fecha
  </th>
  <th>Borrar
  </th>
  
</tr>
<% Object.keys(usuarios).forEach((nombreUsuario)=>{ %>
<tr>
  <td>
    <%=usuarios[nombreUsuario].username %> 
  </td>
  <td>
    <%=usuarios[nombreUsuario].role %> 
  </td>
  <td>
    <%=usuarios[nombreUsuario].last_login %> 
  </td>
  <td>
    <%if(usuarios[nombreUsuario].role !== 'admin'){ %> 
      <form action="/admin/deleteUser" method="post">
        <button name="username" value='<%=usuarios[nombreUsuario].username %>'>delete</button>
      </form> 

  <% } %>
</td>
</tr>


 <% }) %>
<tr>
</tr>





</table>

<form id="formulario">
  <label for="nombre"></label>
  <input type="text" name="name" id="name" placeholder="name" required><br>
  <label for="genero"></label>
  <input type="text" name="genero" id="genero" placeholder="genero" required><br>
  <label for="descripcion"></label>
  <input type="text" name="descripcion" id="descripcion" placeholder="descripcion"><br>
  <button type="submit" form="formulario">Submit</button>
</form>


<%- include("footer", {}) %>